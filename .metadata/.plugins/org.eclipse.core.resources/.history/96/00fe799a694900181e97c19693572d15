package asistentePOO;

import java.util.Date;

public class Evento {
	private String nombre;
	private Date fechaHora;
	
	public Evento(String nombre, Date fechaHora) {
	    this.nombre = nombre;
	    this.fechaHora = fechaHora;
	}
	
	public String getEvento() {
	    return nombre;
	}
	
	public Date getDate() {
	    return fechaHora;
	}
}

