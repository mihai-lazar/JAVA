package p3ab;


public class Smartphone extends User{
	
	String coordenadas;
	
	public Smartphone(String name) {
		super(name);
	}
	
	public String getCoor() {
		return coordenadas;
	}
	
	public void setCoor(String coord) {
		coordenadas = coord;
	}
	
	public void puntos() {
		System.out.println(getName() + " ya tienes " + points);
	}
	

}
