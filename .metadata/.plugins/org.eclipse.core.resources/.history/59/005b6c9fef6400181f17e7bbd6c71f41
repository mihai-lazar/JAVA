package p1a;

import java.util.ArrayList;

public class Jaula {
	
	String id;
	ArrayList<Especie> animales;
	
	public Jaula(String id) {
		this.id = id;
	}
	
	public void addAnimal(Especie especie) {
		animales.add(especie);
	}
	
	public void getAnimal(int i) {
		animales.get(i).showDatos();
	}
	
	public int size() {
		return animales.size();
	}

}
