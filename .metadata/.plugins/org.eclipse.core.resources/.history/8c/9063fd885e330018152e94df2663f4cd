package AvanzeUno;
import java.util.Scanner;


public class InAndOut {
	public static void main(String[] args) {
		String line;
		Scanner in = new Scanner(System.in);
		System.out.print("ESTADO: ");
		line = in.nextLine();
		
		if(line == "OCUPADO") {
			for(int i = 0; i < 10; i++)
			System.out.println("You said: " + line);
		}
		
		if(line == "DESOCUPADO") {
			for(int i = 0; i < 10; i++)
			System.out.println("MENSAJE: " + i);
		}
		
		}
}
